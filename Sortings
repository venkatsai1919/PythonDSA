#Bubble Sort
for i in range(len(arr) - 1):
  for j in range(len(arr) - 1):
    if arr[j] > arr[j + 1]:
      arr[j], arr[j + 1] = arr[j + 1], arr[j]

#selection Sort
for i in range(len(arr)-1):
  min=i
  for j in range(i+1,len(arr)):
    if arr[j]<arr[min]:
      min=j
  arr[i],arr[min]=arr[min],arr[i]
  
  
